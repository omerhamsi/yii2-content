{
  "name": "omerhamsi/yii2content",
  "description": "Yii2 Content Module",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "omerhamsi",
      "email": "omerhamsi@kocaeli.edu.tr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "yiisoft/yii2": "~2.0.13"
  },
  "require-dev": {
    "phpunit/phpunit": "8.*.*"
  },
  "autoload": {
